    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha": "^3.0.0",
    "react-spinners": "^0.13.8",
    "stripe": "^14.2.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
