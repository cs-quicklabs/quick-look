    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "prisma": "^5.4.2",
    "react-error-overlay": "^6.0.11",
    "sass": "^1.69.0",
