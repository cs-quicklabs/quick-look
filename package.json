    "@remix-run/serve": "^2.0.0",
    "@remix-run/v1-route-convention": "^0.1.3",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/forms": "^0.5.6",
    "aws-sdk": "^2.1479.0",
    "axios": "^1.3.5",
